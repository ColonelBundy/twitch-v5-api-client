  "license": "MIT",
  "engines": {
    "node": ">=6.0"
  },
  "dependencies": {
    "request": "^2.79.0"
  },
  "devDependencies": {
    "@types/assert": "0.0.31",
    "@types/jasmine": "^2.5.42",
    "@types/should": "^8.1.30",
    "assert": "^1.4.1",
    "cross-env": "^3.1.3",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "should": "^11.2.0",
    "typescript": "^2.1.6"
  },
  "keywords": [
    "twitch",
    "api",
