{
  "name": "twitch-v5-api-client",
  "version": "0.0.1",
  "description": "Client library to query twitch's v5 api",
  "main": "app.js",
  "scripts": {
    "test": "tsc -p . && mocha dist/app.test.js",
    "watch": "cross-env DEBUG=twitch concurrently -kr \"tsc -w -p .\" \"nodemon dist/app.js --watch dist/ --delay 1000ms\"",
    "dev": "cross-env DEBUG=twitch tsc -p . && nodemon dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/makkesk8/twitch-v5-api-client.git"
  },
